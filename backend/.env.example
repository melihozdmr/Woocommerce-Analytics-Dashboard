# Application
NODE_ENV=development
PORT=3001
FRONTEND_URL=http://localhost:3000
APP_NAME=WooCommerce Analytics

# Database
DATABASE_URL=postgresql://postgres:postgres@localhost:5432/wc_analytics

# Redis
REDIS_HOST=localhost
REDIS_PORT=6379

# JWT
JWT_SECRET=your-jwt-secret-min-32-characters
